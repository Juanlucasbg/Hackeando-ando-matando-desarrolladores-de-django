# Google Maps API Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=staging_google_maps_api_key
REACT_APP_GOOGLE_MAPS_API_URL=https://maps.googleapis.com/maps/api/js

# Environment Configuration
REACT_APP_ENV=staging
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Monitoring and Analytics
REACT_APP_SENTRY_DSN=staging_sentry_dsn
REACT_APP_GA_TRACKING_ID=staging_google_analytics_tracking_id

# API Configuration
REACT_APP_API_URL=https://staging-api.yourdomain.com/api
REACT_APP_API_TIMEOUT=10000

# Feature Flags
REACT_APP_ENABLE_STREET_VIEW=true
REACT_APP_ENABLE_PLACES_AUTOCOMPLETE=true
REACT_APP_ENABLE_GEOCODING=true
REACT_APP_ENABLE_DARK_MODE=true

# Performance Configuration
REACT_APP_MAP_DEFAULT_ZOOM=10
REACT_APP_MAP_MAX_ZOOM=20
REACT_APP_MAP_MIN_ZOOM=2
REACT_APP_DEBOUNCE_DELAY=300

# Security Configuration
REACT_APP_CSP_ENABLED=true
REACT_APP_RATE_LIMIT_ENABLED=true

# Development Settings
REACT_APP_MOCK_API=false
REACT_APP_ENABLE_DEV_TOOLS=true