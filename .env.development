# Development Environment Configuration
VITE_GOOGLE_MAPS_API_KEY=demo_api_key_for_development
VITE_GOOGLE_MAPS_API_URL=https://maps.googleapis.com/maps/api/js
VITE_GOOGLE_MAP_ID=

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:3001
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5
VITE_WEBSOCKET_RECONNECT_DELAY=1000

# Application Configuration
VITE_APP_NAME=Google Maps Clone (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION=A Google Maps clone frontend application - Development

# Environment
VITE_NODE_ENV=development
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_STORYBOOK=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_OFFLINE_SUPPORT=true
VITE_ENABLE_ANALYTICS=false

# Map Configuration
VITE_DEFAULT_LAT=6.2442
VITE_DEFAULT_LNG=-75.5812
VITE_DEFAULT_ZOOM=13
VITE_MAP_MAX_ZOOM=20
VITE_MAP_MIN_ZOOM=3

# Development
VITE_DEV_TOOLS=true
VITE_HOT_RELOAD=true
VITE_SOURCE_MAP=true

# Mock data for development (disabled for real API testing)
VITE_MOCK_API=false
VITE_MOCK_LOCATION_UPDATES=false