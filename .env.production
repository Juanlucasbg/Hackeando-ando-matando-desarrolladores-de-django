# Production Environment Configuration
VITE_GOOGLE_MAPS_API_KEY=your_prod_google_maps_api_key_here
VITE_GOOGLE_MAPS_API_URL=https://maps.googleapis.com/maps/api/js
VITE_GOOGLE_MAP_ID=your_prod_google_map_id_here

# API Configuration
VITE_API_BASE_URL=https://api.movility.ai
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://realtime.movility.ai
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5
VITE_WEBSOCKET_RECONNECT_DELAY=1000

# Application Configuration
VITE_APP_NAME=Google Maps Clone
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=A Google Maps clone frontend application

# Environment
VITE_NODE_ENV=production
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Analytics and Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id_here

# Feature Flags
VITE_ENABLE_STORYBOOK=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_OFFLINE_SUPPORT=true
VITE_ENABLE_ANALYTICS=true

# Map Configuration
VITE_DEFAULT_LAT=6.2442
VITE_DEFAULT_LNG=-75.5812
VITE_DEFAULT_ZOOM=13
VITE_MAP_MAX_ZOOM=20
VITE_MAP_MIN_ZOOM=3

# Development
VITE_DEV_TOOLS=false
VITE_HOT_RELOAD=false
VITE_SOURCE_MAP=false